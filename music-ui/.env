# 环境配置
VUE_APP_TITLE=七洛音乐
VUE_APP_DESCRIPTION=一个现代化的音乐分享平台
VUE_APP_VERSION=1.0.0

# 基础路径配置
BASE_URL=/

# API配置
VUE_APP_API_BASE_URL=/api
# 后端服务地址（用于代理配置）
VUE_APP_BACKEND_URL=http://127.0.0.1:8081/
# 开发服务器端口
VUE_APP_DEV_PORT=8082

# 上传配置
VUE_APP_UPLOAD_MAX_SIZE=50
VUE_APP_UPLOAD_ALLOWED_TYPES=mp3,wav,flac,aac,ogg,m4a

# 图片配置
VUE_APP_IMAGE_MAX_SIZE=5
VUE_APP_IMAGE_ALLOWED_TYPES=jpg,jpeg,png,gif,webp

# 分页配置
VUE_APP_PAGE_SIZE=20
VUE_APP_MAX_PAGE_SIZE=100

# 缓存配置
VUE_APP_CACHE_EXPIRE=3600000

# 第三方服务配置
VUE_APP_GITHUB_CLIENT_ID=your_github_client_id
VUE_APP_GOOGLE_CLIENT_ID=your_google_client_id
VUE_APP_QQ_APP_ID=your_qq_app_id
VUE_APP_WECHAT_APP_ID=your_wechat_app_id

# 开发配置
VUE_APP_DEV_TOOLS=true
VUE_APP_MOCK_API=false